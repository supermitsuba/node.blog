{
    "categories":[
        <% for(var i=0; i < arrayOfCategory.length; i++ ) { %>
            {
                "CategoryName": "<%=arrayOfCategory[i].CategoryType %>",
                "links":
                    {"article":{ "href":"<%='/api/articles?category='+arrayOfCategory[i].CategoryType %>", "name":"<%= 'Articles for '+arrayOfCategory[i].CategoryType %>" } }
                
            }<%= (i < (arrayOfCategory.length-1) ? ',' : '') %>
        <% } %>
    ],
    "links": {
        "self": { "href":"/api/categories" },
        "api":{ "href":"/api" }
    }
}