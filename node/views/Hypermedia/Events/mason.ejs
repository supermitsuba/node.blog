{
    "current": "<%=current %>",
    "fbomb:events":[
        <% for(var i=0; i < arrayOfEvents.length; i++ ) { %>
            {
                "DateOfEvent": "<%=arrayOfEvents[i].DateOfEvent %>",
                "EventDate": "<%=arrayOfEvents[i].EventDate %>",
                "EventName": "<%=arrayOfEvents[i].EventName %>",
                "Sort": "<%=arrayOfEvents[i].Sort %>",
                "@links":
                    {
                        "fbomb:eventLink":
                        { 
                            "href":"<%=arrayOfEvents[i].WebLink %>", "name":"<%=arrayOfEvents[i].EventName %>" 
                        } 
                    }
            }<%= (i < (arrayOfEvents.length-1) ? ',' : '') %>
        <% } %>
    ],
    "@links": {
        "self": { "href":"<%='/api/events?current='+current %>" },
        "fbomb:api":{ "href":"/api" }
    },
    "@namespaces":{
        "fbomb": { "name": "http://fbombcode.com/api/documentation/#{rel}" }
    }
}